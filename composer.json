{
  "name": "0ctobat/octobat-php",
  "description": "Octobat PHP Library",
  "keywords": [
    "octobat",
    "api"
  ],
  "homepage": "https://www.octobat.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Octobat and contributors",
      "homepage": "https://github.com/0ctobat/octobat-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "~0.6.1",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": { "Octobat\\" : "lib/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  }
}
